    "version": "2.0",
    "type": "project",
    "keywords": ["World of Warcraft", "wow", "database", "db", "frontend", "Wrath of the Lich King", "wotlk",  "335a", "3.3.5a"],
    "authors": [
        {
            "name": "Sarjuuk",
            "role": "Developer"
        }
    ],
    "config": {
        "vendor-dir": "includes/libs"
